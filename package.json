    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "babel-jest": "^27.0.2",
    "jest": "^27.0.3",
    "prettier": "2.3.0",
    "typescript": "^4.0.0"
  },
  "jest": {
